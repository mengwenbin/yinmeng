package com.xiaoxu.music.community.entity;

import java.io.Serializable;

public class MusicCommentInfoEntity implements Serializable {
	
	private String comment_id;
	private String user_id;
	private String item_id;
	private String comment_content;
	private String time_create;
	private String ip_create;
	private SimpleUserEntity user;
	
	public MusicCommentInfoEntity() {
		super();
	}

	public MusicCommentInfoEntity(String comment_id, String user_id,
			String item_id, String comment_content, String time_create,
			String ip_create, SimpleUserEntity user) {
		super();
		this.comment_id = comment_id;
		this.user_id = user_id;
		this.item_id = item_id;
		this.comment_content = comment_content;
		this.time_create = time_create;
		this.ip_create = ip_create;
		this.user = user;
	}
	
	public String getComment_id() {
		return comment_id;
	}
	public void setComment_id(String comment_id) {
		this.comment_id = comment_id;
	}
	public String getUser_id() {
		return user_id;
	}
	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}
	public String getItem_id() {
		return item_id;
	}
	public void setItem_id(String item_id) {
		this.item_id = item_id;
	}
	public String getComment_content() {
		return comment_content;
	}
	public void setComment_content(String comment_content) {
		this.comment_content = comment_content;
	}
	public String getTime_create() {
		return time_create;
	}
	public void setTime_create(String time_create) {
		this.time_create = time_create;
	}
	public String getIp_create() {
		return ip_create;
	}
	public void setIp_create(String ip_create) {
		this.ip_create = ip_create;
	}
	public SimpleUserEntity getUser() {
		return user;
	}
	public void setUser(SimpleUserEntity user) {
		this.user = user;
	}
	
	@Override
	public String toString() {
		return "MusicCommentInfoEntity [comment_id=" + comment_id
				+ ", user_id=" + user_id + ", item_id=" + item_id
				+ ", comment_content=" + comment_content + ", time_create="
				+ time_create + ", ip_create=" + ip_create + ", user=" + user
				+ "]";
	}
	
	

}
